<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cache.ecar.com.ecarcache">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="cache.ecar.com.ecarcache.AboutActivity" />
        <activity android:name="cache.ecar.com.ecarcache.SaveBitmapActivity" />
        <activity android:name="cache.ecar.com.ecarcache.SaveJsonObjectActivity" />
        <activity android:name="cache.ecar.com.ecarcache.SaveObjectActivity" />
        <activity android:name="cache.ecar.com.ecarcache.SaveStringActivity" />
        <activity android:name="cache.ecar.com.ecarcache.SaveMediaActivity" />
        <activity android:name="cache.ecar.com.ecarcache.SaveDrawableActivity"/>
        <activity android:name="cache.ecar.com.ecarcache.SaveJsonArrayActivity"/>
    </application>

</manifest>